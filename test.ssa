data $sum_fmt = {b"%d + %d = %d\n", b 0}
data $hello_fmt = {b"Hello\n", b 0}

data $a = { w 10 }
data $b = { w 20 }
data $c = { w 0 }

export function w $main() {
@start
    %a_val = l loadw $a
    %b_val = l loadw $b
    %c_val = w add %a_val, %b_val
    storew %c_val, $c

    call $printf(l $hello_fmt)
    call $printf(l $sum_fmt, w %a_val, w %b_val, w %c_val)

    ret 0
}
